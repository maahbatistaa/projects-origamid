{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": 0,
    "import/extensions": 0,
    "no-console": 0,
    "operator-linebreak": 0,
    "nonblock-statement-body-position": 0,
    "curly": 0
  }
}
